</menu>
<menu id="apps-accessories-menu" label="附件">
  <item label="Squeeze 压缩解压缩工具">
    <action name="Execute">
      <command>squeeze</command>
      <startupnotify>
        <enabled>no</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="Xarchiver 压缩解压缩工具">
    <action name="Execute">
      <command>xarchiver</command>
      <startupnotify>
        <enabled>no</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="Calculator">
    <action name="Execute">
      <command>gnome-calculator</command>
      <startupnotify>
        <enabled>no</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="系统信息">
    <action name="Execute">
      <command>hardinfo</command>
      <startupnotify>
        <enabled>yesg</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="StarDict 辞典">
    <action name="Execute">
      <command>stardict</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="X Kill">
    <action name="Execute">
      <command>xkill</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>

</menu>

